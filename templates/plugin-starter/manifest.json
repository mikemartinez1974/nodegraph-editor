{
  "id": "__PLUGIN_ID__",
  "version": "0.1.0",
  "name": "__PLUGIN_NAME__",
  "description": "Custom nodegraph plugin created with the starter.",
  "permissions": ["graph.read"],
  "bundle": {
    "url": "/plugins/__PLUGIN_SLUG__/plugin.js",
    "sandbox": "iframe",
    "integrity": ""
  },
  "nodes": [
    {
      "type": "__PLUGIN_SLUG__-node",
      "label": "Starter Node",
      "description": "An example node scaffolded by the starter CLI.",
      "category": "custom",
      "icon": "Extension",
      "entry": "/plugins/__PLUGIN_SLUG__/plugin.js#StarterNode",
      "defaultWidth": 240,
      "defaultHeight": 160,
      "defaultData": {
        "title": "Hello from __PLUGIN_NAME__",
        "tagline": "Generated at __CREATED_AT__",
        "enabled": true
      },
      "definition": {
        "size": {
          "width": 240,
          "height": 160
        },
        "handles": {
          "inputs": [
            {
              "id": "trigger",
              "label": "Trigger",
              "dataType": "trigger"
            }
          ],
          "outputs": [
            {
              "id": "result",
              "label": "Result",
              "dataType": "value"
            }
          ]
        },
        "properties": [
          {
            "key": "title",
            "label": "Title",
            "type": "text",
            "default": "Hello from __PLUGIN_NAME__"
          },
          {
            "key": "tagline",
            "label": "Tagline",
            "type": "textarea",
            "placeholder": "Optional subtitle"
          },
          {
            "key": "enabled",
            "label": "Enabled",
            "type": "toggle",
            "default": true
          }
        ],
        "display": {
          "variant": "card",
          "primaryField": "title",
          "secondaryField": "tagline",
          "badgeField": "enabled",
          "emptyState": "Select this node to configure the generated fields."
        }
      }
    }
  ],
  "panels": [],
  "metadata": {
    "homepage": "https://example.com",
    "license": "MIT",
    "changelog": "Initial scaffold generated by the NodeGraph plugin starter",
    "author": {
      "name": "__PLUGIN_NAME__"
    }
  }
}
