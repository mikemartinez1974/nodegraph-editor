{
  "id": "io.breadboard.sockets",
  "version": "0.1.41",
  "name": "Breadboard Socket Toolkit",
  "description": "Built-in plugin that exposes breadboard socket nodes for templates.",
  "permissions": [],
  "bundle": {
    "url": "/plugins/breadboard-sockets/socketPlugin.js",
    "sandbox": "iframe",
    "integrity": ""
  },
  "nodes": [
    {
      "type": "socket",
      "label": "Breadboard Socket",
      "description": "Locked column of breadboard holes used in the board substrate.",
      "category": "breadboard",
      "icon": "FiberManualRecord",
      "entry": "/plugins/breadboard-sockets/socketPlugin.js#SocketNode",
      "defaultWidth": 24,
      "defaultHeight": 63,
      "defaultData": {
        "rows": [
          "A",
          "B",
          "C",
          "D",
          "E"
        ],
        "column": 1,
        "segment": "top"
      },
      "ports": {
        "outputs": [
          {
            "id": "socket",
            "label": "Socket",
            "dataType": "value"
          }
        ]
      },
      "renderer": {
        "entry": "/plugins/breadboard-sockets/socketRenderer.js"
      },
      "extensions": {
        "layout": {
          "hideChrome": true,
          "padding": 0,
          "paddingTop": 0,
          "paddingRight": 0,
          "paddingBottom": 0,
          "paddingLeft": 0
        }
      }
    },
    {
      "type": "railSocket",
      "label": "Rail Socket",
      "description": "Single column of power-rail sockets (two connected holes).",
      "category": "breadboard",
      "icon": "FlashOn",
      "entry": "/plugins/breadboard-sockets/socketPlugin.js#RailSocketNode",
      "defaultWidth": 24,
      "defaultHeight": 54,
      "defaultData": {
        "channel": "top",
        "column": 1,
        "rails": [
          {
            "railId": "rail-top-negative",
            "polarity": "negative",
            "label": "GND",
            "slot": 0
          },
          {
            "railId": "rail-top-positive",
            "polarity": "positive",
            "label": "V+",
            "slot": 1
          }
        ]
      },
      "ports": {
        "outputs": [
          {
            "id": "positive",
            "label": "V+",
            "dataType": "value"
          },
          {
            "id": "negative",
            "label": "GND",
            "dataType": "value"
          }
        ]
      },
      "renderer": {
        "entry": "/plugins/breadboard-sockets/railSocketRenderer.js"
      },
      "extensions": {
        "layout": {
          "hideChrome": true,
          "padding": 0,
          "paddingTop": 0,
          "paddingRight": 0,
          "paddingBottom": 0,
          "paddingLeft": 0
        }
      }
    },
    {
      "type": "skin",
      "label": "Breadboard Skin",
      "description": "Visual baseplate template art that sits under the socket/rail nodes.",
      "category": "breadboard",
      "icon": "GridOn",
      "entry": "/plugins/breadboard-sockets/socketPlugin.js#BoardSkinNode",
      "defaultWidth": 780,
      "defaultHeight": 360,
      "defaultData": {
        "rows": 10,
        "columns": 30,
        "rowPitch": 1,
        "columnPitch": 1,
        "railInset": 32,
        "railThickness": 24,
        "gapHeight": 96
      },
      "renderer": {
        "entry": "/plugins/breadboard-sockets/skinRenderer.js"
      },
      "extensions": {
        "layout": {
          "hideChrome": true,
          "padding": 0,
          "paddingTop": 0,
          "paddingRight": 0,
          "paddingBottom": 0,
          "paddingLeft": 0
        }
      }
    },
    {
      "type": "bus",
      "label": "Breadboard Bus",
      "description": "Invisible rail net that feeds every rail socket column.",
      "category": "breadboard",
      "icon": "CallSplit",
      "entry": "/plugins/breadboard-sockets/socketPlugin.js#BusNode",
      "defaultWidth": 28,
      "defaultHeight": 20,
      "defaultData": {
        "channel": "top",
        "polarity": "positive"
      },
      "ports": {
        "outputs": [
          {
            "id": "positive",
            "label": "V+",
            "dataType": "value"
          },
          {
            "id": "negative",
            "label": "GND",
            "dataType": "value"
          }
        ]
      },
      "renderer": {
        "entry": "/plugins/breadboard-sockets/busRenderer.js"
      },
      "extensions": {
        "layout": {
          "hideChrome": true,
          "padding": 0,
          "paddingTop": 0,
          "paddingRight": 0,
          "paddingBottom": 0,
          "paddingLeft": 0
        }
      }
    }
  ],
  "metadata": {
    "author": {
      "name": "NodeGraph"
    },
    "homepage": "https://example.com/breadboard",
    "license": "MIT"
  }
}